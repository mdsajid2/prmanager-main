# PR Manager Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env files to the repository!

# AI Provider Configuration
AI_PROVIDER=openai
OPENAI_API_KEY=your_openai_api_key_here
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# GEMINI_API_KEY=your_gemini_api_key_here

# GitHub Integration
GITHUB_TOKEN=your_github_personal_access_token_here

# Server Configuration
PORT=3001
NODE_ENV=development

# Database Configuration (Supabase)
DATABASE_URL=postgresql://username:password@host:port/database

# Authentication & Security
JWT_SECRET=your_super_secure_jwt_secret_64_characters_minimum_here
ENCRYPTION_KEY=your_64_character_hex_encryption_key_for_token_storage_here

# Optional: Kiro API (if using)
# KIRO_API_KEY=your_kiro_api_key_here